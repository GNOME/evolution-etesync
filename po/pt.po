#: ../src/credentials/e-credentials-prompter-impl-etesync.c:161
#, c-format
msgid ""
"\n"
"(host: %s)"
msgstr ""
"\n"
"(servidor: %s)"

#: ../src/credentials/e-credentials-prompter-impl-etesync.c:213
msgid "_Cancel"
msgstr "_Cancelar"

#: ../src/credentials/e-credentials-prompter-impl-etesync.c:214
msgid "_OK"
msgstr "_OK"


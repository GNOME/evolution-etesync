#: ../src/credentials/e-credentials-prompter-impl-etesync.c:492
msgid "_Cancel"
msgstr "A_vbryt"

#: ../src/credentials/e-credentials-prompter-impl-etesync.c:269
#: ../src/credentials/e-credentials-prompter-impl-etesync.c:493
msgid "_OK"
msgstr "_OK"

